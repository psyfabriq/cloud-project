    <parent>
        <groupId>ru-psyfabriq-borderline</groupId>
        <artifactId>side-borderline</artifactId>
        <version>0.0.1</version>
    </parent>

    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
